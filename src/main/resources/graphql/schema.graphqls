# Root
type Query {
    users: [User]
    user(id: Int!): User
    countUsers: Int
}

type User {
    id: ID
    nombre: String
    apellido: String
    email:String
}

# Root
type Mutation {
    createUser(nombre: String, apellido: String, email: String): User
    updateUser(id: Int!, nombre: String, apellido: String, email: String): User
    deleteUser(id: Int!): User
}


